# Copy to next-app/.env.local and fill in values.
# See next-app/.env.example for the full list.

# Clerk (https://dashboard.clerk.com)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Database (PostgreSQL – e.g. Neon, local)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require

# Ollama (chat backend – must be reachable from the app)
OLLAMA_BASE_URL=http://localhost:11434
